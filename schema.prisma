datasource db {
  provider = "postgres"

  // .envファイルからデータベースURLを読み込み
  url = env("DATABASE_URL")
}
 
generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}
 
model Stock {
  id Int @id @unique @default(autoincrement())
  name String
  num Int
}

model Order {
  id Int @id @unique @default(autoincrement())
  customer Int
  product Int
  num Int
  time DateTime @db.Timestamptz()
}

model EditInfo {
  id Int @id @unique @default(autoincrement())
  table String
  type Int
  info Json
  time DateTime @db.Timestamptz()
}