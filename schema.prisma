datasource db {
  provider = "sqlite"
  url      = "file:./db/dev.sqlite"
}


generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}

model Stock {
  id Int @id @unique @default(autoincrement())
  name String
  num Int
}

model Order {
  id Int @id @unique @default(autoincrement())
  customer Int
  product Int
  start DateTime
  end DateTime
  num Int
  time DateTime
  state Int
  note String
}

model EditInfo {
  id Int @id @unique @default(autoincrement())
  table String
  type Int
  info String // Json 型から String 型に変更
  time DateTime
}
